{
  "name": "readergata",
  "version": "0.1.0",
  "private": true,
  "homepage": "",
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/roboto": "^4.5.8",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@piotr-cz/redux-persist-idb-storage": "^1.1.3",
    "@reduxjs/toolkit": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/dompurify": "^2.4.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-pdf": "^6.2.0",
    "axios": "^1.1.3",
    "babel-plugin-import": "^1.13.3",
    "dexie": "^3.2.2",
    "dompurify": "^2.4.2",
    "epubjs": "^0.3.93",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "install": "^0.13.0",
    "notistack": "^2.0.8",
    "plugin-frame": "^0.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^12.0.0",
    "react-pdf": "^6.2.1",
    "react-query": "^3.39.2",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.2.1",
    "redux-persist": "^6.0.0",
    "screenfull": "^5.0.2",
    "source-map-explorer": "^2.4.2",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "copy-worker": "node ./scripts/copy-worker.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "cors-server": "node servers/cors/server.js",
    "postinstall": "cd servers/cors && npm install",
    "dev": "concurrently --kill-others \"npm run cors-server\" \"npm start\""
  },
  "eslintConfig": {
    "plugins": [
      "jam3",
      "i18next"
    ],
    "rules": {
      "jam3/no-sanitizer-with-danger": 2,
      "i18next/no-literal-string": [
        2,
        {
          "words": {
            "exclude": [
              "[0-9!-/:-@[-`{-~]+",
              "[A-Z_-]+"
            ]
          }
        }
      ]
    },
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-redux": "^7.1.11",
    "@types/webpack-env": "^1.16.0",
    "concurrently": "^5.3.0",
    "customize-cra": "^1.0.0",
    "eslint-plugin-i18next": "^6.0.0-6",
    "eslint-plugin-jam3": "^0.2.3",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "5.0.1"
  }
}
